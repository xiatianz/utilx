;;;AA3BEC,6BAAAF,QAAAG,YAAAC,wBAAA,iBAAiB,KAAK,IAAI,CAAA,GAAA,EAC/B,OAAM,qCAAoC,GAAA,IAAA,GAAAH,UAAA,QAAA;gIAGMI,eAAA,KAAK,IAAI,6EACbA,eAAA,KAAK,WAAW;AAKtD,kBAAA,KAAK,KAAG;;;;;AAMR,kBAAA,KAAK,KAAG;;;;;+NAQ0B,gBAAgB,KAAK,QAAQ,CAAA,2DAChE,gBAAgB,KAAK,SAAS,CAAA;;;gBA5BzCF,YAyBM,OAAA,EAzBD,OAAM,wCAAuC,GAAA;AAAA,kBAChDA,YASM,OAAA,EATD,OAAM,0BAAyB,GAAA;AAAA,kCAClCG,YAGEF,wBAFK,iBAAiB,KAAK,IAAI,CAAA,GAAA,EAC/B,OAAM,sCAAoC;AAAA,oBAE5CD,YAGM,OAAA,MAAA;AAAA,sBAFJA,YAAiE,QAA7D,OAAM,mCAAkC,GAAAI,gBAAI,KAAK,IAAI,GAAA,CAAA;AAAA,sBACzDJ,YAAmE,OAAhE,OAAM,gCAA+B,GAAAI,gBAAI,KAAK,WAAW,GAAA,CAAA;AAAA;;kBAGhEJ,YAaM,OAAA,EAbD,OAAM,aAAY,GAAA;AAAA,oBAEb,KAAK,oBADbG,YAKO,QAAA;AAAA;sBAHL,OAAM;AAAA,uBACP,OAED;oBAEQ,KAAK,oBADbA,YAKO,QAAA;AAAA;sBAHL,OAAM;AAAA,uBACP,OAED;;;gBAGJH,YAGM,OAAA,EAHD,OAAM,kEAAiE,GAAA;AAAA,kBAC1EA,YAAoF,QAAA,EAA9E,OAAM,6BAA4B,mBAAI,gBAAgB,KAAK,QAAQ,CAAA,GAAA,CAAA;AAAA,kBACzEA,YAAkD,QAAA,MAAAI,gBAAzC,gBAAgB,KAAK,SAAS,CAAA,GAAA,CAAA;AAAA;;;;;;;;AAMlC,UAAA,cAAA,MAAc,WAAM,GAAA;;AAEnB,cAAAV,mBAAAC,MAAA,MAAA,GAAA,EAAA,OAAM,gCAA+B,GAAA,MAAA,OAAA,CAAA;;;;;;AASrC,YAAAD,mBAAAC,MAAA,QAAA,GAAA,EAAA,OAAM,uBAAsB,GAAA,MAAA,OAAA,CAAA;;;QAKpC,IAAG;AAAA,QACH,OAAM;AAAA;yBAFR,CAMW,GAAAE,QAAAC,UAAA,aAAA;;;;;cAFTE,YAAsD,MAAA,EAAlD,OAAM,mCAAkC,GAAC,MAAI;AAAA,cACjDA,YAA8D,KAAA,EAA3D,OAAM,gCAA+B,GAAC,mBAAiB;AAAA;;;;;;QAG1D,IAAG;AAAA,QACH,OAAM;AAAA;yBAFR,CAMW,GAAAH,QAAAC,UAAA,aAAA;;;;;cAFTE,YAAsD,MAAA,EAAlD,OAAM,mCAAkC,GAAC,MAAI;AAAA,cACjDA,YAAqD,KAAA,EAAlD,OAAM,gCAA+B,GAAC,UAAQ;AAAA;;;;;;QAGjD,IAAG;AAAA,QACH,OAAM;AAAA;yBAFR,CAMW,GAAAH,QAAAC,UAAA,aAAA;;;;;cAFTE,YAAsD,MAAA,EAAlD,OAAM,mCAAkC,GAAC,MAAI;AAAA,cACjDA,YAAoD,KAAA,EAAjD,OAAM,gCAA+B,GAAC,SAAO;AAAA;;;;;;QAGhD,IAAG;AAAA,QACH,OAAM;AAAA;yBAFR,CAMW,GAAAH,QAAAC,UAAA,aAAA;;;;;cAFTE,YAAuD,MAAA,EAAnD,OAAM,mCAAkC,GAAC,OAAK;AAAA,cAClDA,YAAqD,KAAA,EAAlD,OAAM,gCAA+B,GAAC,UAAQ;AAAA;;;;;+ZAYlCE,eAAAP,MAAA,KAAA,EAAM,MAAM,qCACZO,eAAAP,MAAA,UAAA,EAAW,MAAM,qCACjBO,eAAAP,MAAA,KAAA,EAAM,SAASA,MAAA,UAAA,EAAW,SAAM,CAAA;AAatC,YAAAD,mBAAAC,MAAA,KAAA,GAAA,EAAA,OAAM,UAAS,GAAA,MAAA,OAAA,CAAA;;;;;;;;;;;;","names":["_ssrRenderAttrs","_mergeProps","_ssrRenderComponent","_unref","_ssrRenderList","_push","_parent","_ssrRenderVNode","_createVNode","_resolveDynamicComponent","_ssrInterpolate","_createBlock","_toDisplayString"],"sources":["../../../../src/pages/sitemap.vue"],"sourcesContent":["<template>\n  <div class=\"max-w-4xl mx-auto py-8\">\n    <div class=\"mb-8\">\n      <h1 class=\"text-3xl font-bold text-foreground mb-2\">站点地图</h1>\n      <p class=\"text-muted-foreground\">Util.cn 完整的网站页面索引</p>\n    </div>\n\n    <!-- 主要导航 -->\n    <section class=\"mb-12\">\n      <h2 class=\"text-xl font-semibold mb-4 flex items-center gap-2\">\n        <Home class=\"w-5 h-5 text-primary\" />\n        主要页面\n      </h2>\n      <div class=\"grid grid-cols-1 md:grid-cols-2 gap-4\">\n        <a\n          href=\"/\"\n          class=\"block p-4 bg-card border border-border rounded-lg hover:border-primary/50 transition-colors\"\n        >\n          <h3 class=\"font-medium text-foreground mb-1\">首页</h3>\n          <p class=\"text-sm text-muted-foreground\">开发者效率工具箱首页</p>\n        </a>\n        <a\n          href=\"/all\"\n          class=\"block p-4 bg-card border border-border rounded-lg hover:border-primary/50 transition-colors\"\n        >\n          <h3 class=\"font-medium text-foreground mb-1\">全部工具</h3>\n          <p class=\"text-sm text-muted-foreground\">浏览所有可用工具</p>\n        </a>\n        <a\n          href=\"/explore\"\n          class=\"block p-4 bg-card border border-border rounded-lg hover:border-primary/50 transition-colors\"\n        >\n          <h3 class=\"font-medium text-foreground mb-1\">工具探索</h3>\n          <p class=\"text-sm text-muted-foreground\">发现和探索工具</p>\n        </a>\n        <a\n          href=\"/ai\"\n          class=\"block p-4 bg-card border border-border rounded-lg hover:border-primary/50 transition-colors\"\n        >\n          <h3 class=\"font-medium text-foreground mb-1\">AI工具导航</h3>\n          <p class=\"text-sm text-muted-foreground\">AI智能工具合集</p>\n        </a>\n      </div>\n    </section>\n\n    <!-- 分类页面 -->\n    <section class=\"mb-12\">\n      <h2 class=\"text-xl font-semibold mb-4 flex items-center gap-2\">\n        <LayoutGrid class=\"w-5 h-5 text-primary\" />\n        工具分类\n      </h2>\n      <div class=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4\">\n        <NuxtLink\n          v-for=\"category in categories\"\n          :key=\"category.id\"\n          :to=\"'/' + category.id\"\n          class=\"block p-4 bg-card border border-border rounded-lg hover:border-primary/50 transition-colors\"\n        >\n          <div class=\"flex items-center gap-3 mb-2\">\n            <component\n              :is=\"getIconComponent(category.icon)\"\n              class=\"w-6 h-6 text-primary\"\n            />\n            <h3 class=\"font-medium text-foreground\">{{ category.name }}</h3>\n          </div>\n          <p class=\"text-sm text-muted-foreground\">{{ category.description }}</p>\n        </NuxtLink>\n      </div>\n    </section>\n\n    <!-- 工具列表 -->\n    <section class=\"mb-12\">\n      <h2 class=\"text-xl font-semibold mb-4 flex items-center gap-2\">\n        <Wrench class=\"w-5 h-5 text-primary\" />\n        工具列表 ({{ tools.length }} 个)\n      </h2>\n      <div class=\"mb-4\">\n        <input\n          v-model=\"searchQuery\"\n          type=\"text\"\n          placeholder=\"搜索工具...\"\n          class=\"w-full md:w-96 px-4 py-2 bg-muted border border-border rounded-lg focus:outline-none focus:ring-2 focus:ring-primary focus:bg-background\"\n        >\n      </div>\n\n      <div class=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4\">\n        <NuxtLink\n          v-for=\"tool in filteredTools\"\n          :key=\"tool.id\"\n          :to=\"`/tools/${tool.id}/`\"\n          class=\"block p-4 bg-card border border-border rounded-lg hover:border-primary/50 transition-colors\"\n        >\n          <div class=\"flex items-start justify-between mb-2\">\n            <div class=\"flex items-center gap-3\">\n              <component\n                :is=\"getIconComponent(tool.icon)\"\n                class=\"w-6 h-6 text-primary flex-shrink-0\"\n              />\n              <div>\n                <h3 class=\"font-medium text-foreground mb-1\">{{ tool.name }}</h3>\n                <p class=\"text-sm text-muted-foreground\">{{ tool.description }}</p>\n              </div>\n            </div>\n            <div class=\"flex gap-1\">\n              <span\n                v-if=\"tool.hot\"\n                class=\"text-xs px-2 py-1 bg-red-500/20 text-red-400 rounded-full\"\n              >\n                HOT\n              </span>\n              <span\n                v-if=\"tool.new\"\n                class=\"text-xs px-2 py-1 bg-green-500/20 text-green-400 rounded-full\"\n              >\n                NEW\n              </span>\n            </div>\n          </div>\n          <div class=\"flex items-center justify-between text-xs text-muted-foreground\">\n            <span class=\"px-2 py-1 bg-muted rounded\">{{ getCategoryName(tool.category) }}</span>\n            <span>{{ formatViewCount(tool.viewCount) }}</span>\n          </div>\n        </NuxtLink>\n      </div>\n\n      <!-- 搜索结果为空 -->\n      <div v-if=\"filteredTools.length === 0\" class=\"text-center py-12\">\n        <div class=\"inline-flex items-center justify-center w-16 h-16 rounded-full bg-muted mb-4\">\n          <Search class=\"w-8 h-8 text-muted-foreground\" />\n        </div>\n        <p class=\"text-muted-foreground\">未找到匹配的工具</p>\n      </div>\n    </section>\n\n    <!-- 其他页面 -->\n    <section class=\"mb-12\">\n      <h2 class=\"text-xl font-semibold mb-4 flex items-center gap-2\">\n        <FileText class=\"w-5 h-5 text-primary\" />\n        其他页面\n      </h2>\n      <div class=\"grid grid-cols-1 md:grid-cols-2 gap-4\">\n        <NuxtLink\n          to=\"/about\"\n          class=\"block p-4 bg-card border border-border rounded-lg hover:border-primary/50 transition-colors\"\n        >\n          <h3 class=\"font-medium text-foreground mb-1\">关于我们</h3>\n          <p class=\"text-sm text-muted-foreground\">了解 Util.cn 和我们的使命</p>\n        </NuxtLink>\n        <NuxtLink\n          to=\"/faq\"\n          class=\"block p-4 bg-card border border-border rounded-lg hover:border-primary/50 transition-colors\"\n        >\n          <h3 class=\"font-medium text-foreground mb-1\">常见问题</h3>\n          <p class=\"text-sm text-muted-foreground\">快速解答常见疑问</p>\n        </NuxtLink>\n        <NuxtLink\n          to=\"/feedback\"\n          class=\"block p-4 bg-card border border-border rounded-lg hover:border-primary/50 transition-colors\"\n        >\n          <h3 class=\"font-medium text-foreground mb-1\">用户反馈</h3>\n          <p class=\"text-sm text-muted-foreground\">提交意见和建议</p>\n        </NuxtLink>\n        <NuxtLink\n          to=\"/download\"\n          class=\"block p-4 bg-card border border-border rounded-lg hover:border-primary/50 transition-colors\"\n        >\n          <h3 class=\"font-medium text-foreground mb-1\">桌面版下载</h3>\n          <p class=\"text-sm text-muted-foreground\">下载离线桌面应用</p>\n        </NuxtLink>\n      </div>\n    </section>\n\n    <!-- SEO 信息 -->\n    <section class=\"mt-16 p-6 bg-muted/30 rounded-lg\">\n      <h2 class=\"text-lg font-semibold mb-4\">SEO 信息</h2>\n      <div class=\"grid grid-cols-1 md:grid-cols-2 gap-6 text-sm\">\n        <div>\n          <h3 class=\"font-medium text-foreground mb-2\">统计信息</h3>\n          <ul class=\"space-y-1 text-muted-foreground\">\n            <li>工具总数: {{ tools.length }} 个</li>\n            <li>分类数量: {{ categories.length }} 个</li>\n            <li>总页面数: {{ tools.length + categories.length + 8 }} 个</li>\n          </ul>\n        </div>\n        <div>\n          <h3 class=\"font-medium text-foreground mb-2\">XML 站点地图</h3>\n          <p class=\"text-muted-foreground mb-2\">\n            搜索引擎可以通过以下地址访问我们的 XML 站点地图：\n          </p>\n          <a\n            href=\"/sitemap.xml\"\n            target=\"_blank\"\n            class=\"inline-flex items-center gap-2 px-4 py-2 bg-background border border-border rounded-lg hover:bg-muted transition-colors\"\n          >\n            <Globe class=\"w-4 h-4\" />\n            /sitemap.xml\n          </a>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from 'vue'\nimport {\n  Home, LayoutGrid, Wrench, Search, FileText, Globe,\n  FileJson, Database, Link, Hash, Timer, Regex, FileDiff,\n  FolderOpen, GitBranch, Type, Wifi, Image, Code,\n  Shield, Layout, Clock, Lock\n} from 'lucide-vue-next'\nimport { categories } from '~/data/categories'\nimport { tools } from '~/data/tools'\nimport { useSEO } from '~/composables/useSEO'\n\n// 设置页面标题\nconst { setPageTitle } = useSEO()\nsetPageTitle('站点地图')\n\ndefinePageMeta({\n  layout: 'default'\n})\n\n// 搜索功能\nconst searchQuery = ref('')\n\n// 图标映射\nconst iconMap = {\n  FileText: FileText,\n  Lock: Shield,\n  Clock: Clock,\n  Type: Type,\n  Wifi: Wifi,\n  Image: Image,\n  Code: Code,\n  FileJson: FileJson,\n  Database: Database,\n  Link: Link,\n  Hash: Hash,\n  Timer: Timer,\n  Regex: Regex,\n  FileDiff: FileDiff,\n  Globe: Globe,\n  FolderOpen: FolderOpen,\n  GitBranch: GitBranch,\n  Layout: Layout\n}\n\n// 获取图标组件\nconst getIconComponent = (iconName) => {\n  return iconMap[iconName] || FileText\n}\n\n// 获取分类名称\nconst getCategoryName = (categoryId) => {\n  const category = categories.find(c => c.id === categoryId)\n  return category ? category.name : categoryId\n}\n\n// 格式化浏览量\nconst formatViewCount = (count) => {\n  if (count >= 10000) {\n    return `${(count / 10000).toFixed(1)}w+`\n  } else if (count >= 1000) {\n    return `${(count / 1000).toFixed(1)}k+`\n  }\n  return `${count}`\n}\n\n// 过滤工具\nconst filteredTools = computed(() => {\n  if (!searchQuery.value.trim()) {\n    return tools\n  }\n\n  const query = searchQuery.value.toLowerCase()\n  return tools.filter(tool =>\n    tool.name.toLowerCase().includes(query) ||\n    tool.description.toLowerCase().includes(query) ||\n    tool.keywords?.some(keyword => keyword.toLowerCase().includes(query))\n  )\n})\n</script>\n\n<style scoped>\n/* 站点地图样式 */\n.grid {\n  display: grid;\n}\n\n/* 链接悬停效果 */\na {\n  text-decoration: none;\n  color: inherit;\n}\n\n/* 响应式调整 */\n@media (max-width: 768px) {\n  .grid-cols-1.md\\\\:grid-cols-2 {\n    grid-template-columns: repeat(1, minmax(0, 1fr));\n  }\n\n  .grid-cols-1.md\\\\:grid-cols-2.lg\\\\:grid-cols-3 {\n    grid-template-columns: repeat(1, minmax(0, 1fr));\n  }\n}\n</style>"],"version":3}