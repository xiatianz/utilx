{"file":"index-Dm55oVw5.js","mappings":";;;;;;;;;;;;;;;;;;;;;;;;AAsPA,UAAM,EAAE,aAAA,IAAiB,OAAA;AACzB,iBAAa,oBAAoB;AAGlB,cAAA;AAGf,kBAAA;AAGA,UAAM,cAAc,IAAI,EAAE;AAC1B,UAAM,sBAAsB,IAAI,IAAI;AAGpC,UAAM,UAAU;AAAA,MACd;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,IAAA;AAIF,UAAM,cAAc,IAAI;AAAA,MACtB;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,MACA;AAAA,IAAA,CACD;AAGD,UAAM,mBAAmB,SAAS,MAAM;AAEtC,aAAO,MAAM,OAAO,CAAA,SAAQ,KAAK,GAAG,EAAE,MAAM,GAAG,CAAC;AAAA,IAClD,CAAC;AAGD,UAAM,cAAc,SAAS,MAAM;AACjC,aAAO,iBACJ,IAAI,CAAA,OAAM,MAAM,KAAK,CAAA,SAAQ,KAAK,OAAO,EAAE,CAAC,EAC5C,OAAO,OAAO,EACd,MAAM,GAAG,CAAC;AAAA,IACf,CAAC;AAGD,UAAM,sBAAsB,SAAS,MAAM;AACzC,aAAO,WAAW,IAAI,CAAA,cAAa;AAAA,QACjC,GAAG;AAAA,QACH,OAAO,MAAM,OAAO,UAAQ,KAAK,aAAa,SAAS,EAAE;AAAA,MAAA,EACzD;AAAA,IACJ,CAAC;AAGmB,QAAI,IAAI;AA4C5B,UAAM,mBAAmB,CAAC,aAAa;AACrC,YAAM,cAAc,QAAQ,UAAU,KAAK,QAAQ,UAAU;AAC7D,aAAO,QAAQ,QAAQ,KAAK;AAAA,IAC9B;AAMA,UAAMA,qBAAmBC;;;AA/WlB,YAAA,OAAAC,eAAAC,WAAA,EAAA,OAAM,4BAAwB,MAAA,CAAA,CAAA,qgDAAA;AA2BnB,YAAAC,mBAAAC,MAAA,MAAA,GAAA,EAAA,OAAM,gCAAA,GAA+B,MAAA,OAAA,CAAA;kDAIpC,YAAA,KAAW,CAAA,64BAAA;UAiBhB,oBAAA,OAAmB;sPAIgBC,eAAA,oBAAA,MAAoB,IAAI,6FAKrDA,eAAA,oBAAA,MAAoB,QAAQ;AAOnC,cAAAF,mBAAAC,MAAA,CAAA,GAAA,EAAA,OAAM,UAAA,GAAS,MAAA,OAAA,CAAA;;;;;;AAQJE,oBAAA,YAAA,QAAP,QAAG;wQAKP,GAAG,CAAA,WAAA;AAAA;;AAUC,YAAAH,mBAAAC,MAAA,KAAA,GAAA,EAAA,OAAM,uBAAA,GAAsB,MAAA,OAAA,CAAA;;AAI7B,UAAA,YAAA,MAAY,SAAM,GAAA;;AAIhB,cAAAD,mBAAAC,MAAA,MAAA,GAAA,EAAA,OAAM,UAAA,GAAS,MAAA,OAAA,CAAA;;;;;;AAKhB,UAAA,YAAA,MAAY,SAAM,GAAA;;AAEVE,sBAAA,YAAA,QAAR,SAAI;;YACV,KAAK,KAAK;AAAA,YACV,IAAE,UAAY,KAAK,EAAE;AAAA,YACtB,OAAM;AAAA,YACL,SAAK,CAAA,WAAEF,MAAA,aAAA,EAAc,KAAK,EAAE;AAAA,UAAA;6BAL/B,CAiBW,GAAAG,QAAAC,UAAA,aAAA;;;2EAPE,iBAAiB,KAAK,IAAI,CAAA,GAAA;AAAA,kBAC/B,OAAM;AAAA,kBACL,gBAAgBJ,MAAAL,kBAAA,EAAiB,KAAK,QAAQ,EAAE,KAAA;AAAA,gBAAI;uGAEFM,eAAA,KAAK,IAAI;;;kBAPlEI,YASM,OAAA,EATD,OAAM,yKAAqK;AAAA,oBAC9KA,YAOM,OAAA,EAPD,OAAM,mDAA+C;AAAA,uBACxDC,UAAA,GAAAC,YAIEC,wBAHK,iBAAiB,KAAK,IAAI,CAAA,GAAA;AAAA,wBAC/B,OAAM;AAAA,wBACL,gBAAgBR,MAAAL,kBAAA,EAAiB,KAAK,QAAQ,EAAE,KAAA;AAAA,sBAAI;sBAEvDU,YAAwE,UAAlE,OAAM,sCAAA,GAAqCI,gBAAI,KAAK,IAAI,GAAA,CAAA;AAAA,oBAAA;;;;;;;;;;;AASnDP,sBAAA,iBAAA,QAAR,SAAI;;YACV,KAAK,KAAK;AAAA,YACV,IAAE,UAAY,KAAK,EAAE;AAAA,YACtB,OAAM;AAAA,YACL,SAAK,CAAA,WAAEF,MAAA,aAAA,EAAc,KAAK,EAAE;AAAA,UAAA;6BAL/B,CAiBW,GAAAG,QAAAC,UAAA,aAAA;;;2EAPE,iBAAiB,KAAK,IAAI,CAAA,GAAA;AAAA,kBAC/B,OAAM;AAAA,kBACL,gBAAgBJ,MAAAL,kBAAA,EAAiB,KAAK,QAAQ,EAAE,KAAA;AAAA,gBAAI;uGAEFM,eAAA,KAAK,IAAI;;;kBAPlEI,YASM,OAAA,EATD,OAAM,yKAAqK;AAAA,oBAC9KA,YAOM,OAAA,EAPD,OAAM,mDAA+C;AAAA,uBACxDC,UAAA,GAAAC,YAIEC,wBAHK,iBAAiB,KAAK,IAAI,CAAA,GAAA;AAAA,wBAC/B,OAAM;AAAA,wBACL,gBAAgBR,MAAAL,kBAAA,EAAiB,KAAK,QAAQ,EAAE,KAAA;AAAA,sBAAI;sBAEvDU,YAAwE,UAAlE,OAAM,sCAAA,GAAqCI,gBAAI,KAAK,IAAI,GAAA,CAAA;AAAA,oBAAA;;;;;;;;;;;AAUjDP,oBAAA,oBAAA,QAAZ,aAAQ;8DAGd,MAAE,YAAc,SAAS,EAAE,EAAA,CAAA,+FAAA;kEAInB,iBAAiB,SAAS,IAAI,CAAA,GAAA;AAAA,UACnC,OAAM;AAAA,UACL,gBAAgBF,MAAAL,kBAAA,EAAiB,SAAS,EAAE,EAAE,KAAA;AAAA,QAAI;AAElD,cAAA,IAAAM,eAAA,SAAS,IAAI,CAAA,iGAAA;AAKCC,sBAAA,SAAS,MAAM,cAAvB,SAAI;;YACV,KAAK,KAAK;AAAA,YACV,IAAE,UAAY,KAAK,EAAE;AAAA,YACtB,OAAM;AAAA,YACL,SAAK,CAAA,WAAEF,MAAA,aAAA,EAAc,KAAK,EAAE;AAAA,UAAA;6BAL/B,CA2BW,GAAAG,QAAAC,UAAA,aAAA;;;2EAjBE,iBAAiB,KAAK,IAAI,CAAA,GAAA;AAAA,kBAC/B,OAAM;AAAA,kBACL,gBAAgBJ,MAAAL,kBAAA,EAAiB,KAAK,QAAQ,EAAE,KAAA;AAAA,gBAAI;AAG/C,oBAAA,KAAK,OAAO,KAAK,KAAG;;;oBAC0D,KAAK,MAAG,+BAAA;AAAA,kBAAA,kCAKzFM,eAAA,KAAK,MAAG,QAAA,KAAA;;;;0FAGmBA,eAAA,KAAK,IAAI,6EACCA,eAAA,KAAK,WAAW;;;kBAlB9DI,YAmBM,OAAA,EAnBD,OAAM,yKAAqK;AAAA,oBAC9KA,YAeM,OAAA,EAfD,OAAM,2CAAuC;AAAA,uBAChDC,UAAA,GAAAC,YAIEC,wBAHK,iBAAiB,KAAK,IAAI,CAAA,GAAA;AAAA,wBAC/B,OAAM;AAAA,wBACL,gBAAgBR,MAAAL,kBAAA,EAAiB,KAAK,QAAQ,EAAE,KAAA;AAAA,sBAAI;sBAG/C,KAAK,OAAO,KAAK,oBADzBY,YAQO,QAAA;AAAA;wBANJ,OAAK;AAAA;0BAA8E,KAAK,MAAG,+BAAA;AAAA,wBAAA;AAAA,yBAKzFE,gBAAA,KAAK,MAAG,QAAA,KAAA,GAAA,CAAA;;oBAGfJ,YAAmD,QAA/C,OAAM,qBAAA,GAAoBI,gBAAI,KAAK,IAAI,GAAA,CAAA;AAAA,oBAC3CJ,YAAmE,OAAhE,OAAM,gCAAA,GAA+BI,gBAAI,KAAK,WAAW,GAAA,CAAA;AAAA,kBAAA;;;;;;;;;;AAYxD,YAAAV,mBAAAC,MAAA,IAAA,GAAA,EAAA,OAAM,uBAAA,GAAsB,MAAA,OAAA,CAAA;;AAQ7B,YAAAD,mBAAAC,MAAA,GAAA,GAAA,EAAA,OAAM,uBAAA,GAAsB,MAAA,OAAA,CAAA;;AAQ1B,YAAAD,mBAAAC,MAAA,KAAA,GAAA,EAAA,OAAM,uBAAA,GAAsB,MAAA,OAAA,CAAA;;;;;;;;;;;;","names":["getCategoryColor","getCatColor","_ssrRenderAttrs","_mergeProps","_ssrRenderComponent","_unref","_ssrInterpolate","_ssrRenderList","_push","_parent","_createVNode","_openBlock","_createBlock","_resolveDynamicComponent","_toDisplayString"],"sources":["../../../../src/pages/index.vue"],"sourcesContent":["<template>\n  <div class=\"max-w-8xl mx-auto py-8\">\n    <!-- Hero 区域 -->\n    <div class=\"relative text-center mb-8 mt-4 overflow-hidden rounded-2xl bg-gradient-to-br from-primary/5 via-transparent to-secondary/5\">\n      <!-- 呼吸极光背景效果 -->\n      <div class=\"absolute inset-0 -z-10\">\n        <!-- 主要呼吸光层 -->\n        <div class=\"absolute top-1/4 left-1/3 w-64 h-64 bg-primary/20 rounded-full filter blur-2xl animate-breathe\"></div>\n        <div class=\"absolute bottom-1/4 right-1/3 w-72 h-72 bg-secondary/15 rounded-full filter blur-2xl animate-breathe delay-2000\"></div>\n        <div class=\"absolute top-1/2 left-1/2 w-56 h-56 bg-accent/10 rounded-full filter blur-2xl animate-breathe delay-4000\"></div>\n\n        <!-- 细节光点层 -->\n        <div class=\"absolute top-1/6 left-1/4 w-32 h-32 bg-primary/15 rounded-full filter blur-xl animate-pulse-slow\"></div>\n        <div class=\"absolute bottom-1/6 right-1/4 w-40 h-40 bg-secondary/10 rounded-full filter blur-xl animate-pulse-slow delay-1000\"></div>\n\n        <!-- 微光渐变叠加 -->\n        <div class=\"absolute inset-0 bg-gradient-to-t from-background/50 via-transparent to-background/30\"></div>\n      </div>\n\n      <div class=\"relative z-10 px-6 py-8 lg:px-8 lg:py-12\">\n        <h1 class=\"text-3xl lg:text-4xl font-bold mb-4 bg-gradient-to-r from-foreground to-muted-foreground bg-clip-text text-transparent\">\n          不仅是工具，更是创造力的延伸\n        </h1>\n        <p class=\"text-lg text-muted-foreground mb-6\">极简、高效、全能。为极客、设计师和所有探索者打造。</p>\n\n        <!-- 大搜索框 -->\n        <div class=\"relative max-w-2xl mx-auto mb-6\">\n        <div class=\"absolute inset-y-0 left-0 pl-4 flex items-center pointer-events-none z-10\">\n          <Search class=\"w-5 h-5 text-muted-foreground\" />\n        </div>\n        <input\n          ref=\"searchInput\"\n          v-model=\"searchQuery\"\n          type=\"text\"\n          class=\"block w-full rounded-xl border border-border/60 bg-card/40 backdrop-blur-sm pl-11 pr-24 py-4 text-base focus:outline-none focus:ring-2 focus:ring-primary/50 focus:bg-background/95 transition-all placeholder:text-muted-foreground/60\"\n          placeholder=\"搜索你需要的工具，如 'JSON', 'MD5', '正则'  - Extend Your Creativity\"\n          @input=\"handleSearch\"\n          @keydown.enter=\"handleSearchEnter\"\n        >\n        <div class=\"absolute inset-y-0 right-0 pr-3 flex items-center\">\n          <span class=\"text-xs text-muted-foreground/60 font-mono bg-background/80 backdrop-blur-sm border border-border/40 px-2 py-1.5 rounded-lg shadow-sm hidden sm:inline-flex items-center gap-1\">\n            <kbd class=\"text-[11px] font-semibold bg-muted/50 px-1.5 py-0.5 rounded border border-border/30\">⌘</kbd>\n            <kbd class=\"text-[11px] font-semibold bg-muted/50 px-1.5 py-0.5 rounded border border-border/30\">K</kbd>\n          </span>\n        </div>\n      </div>\n      \n      <!-- 智能粘贴提示 -->\n      <div \n        v-if=\"clipboardSuggestion\" \n        class=\"max-w-2xl mx-auto mb-6 p-4 bg-primary/10 border border-primary/20 rounded-lg flex items-center justify-between\"\n      >\n        <div class=\"text-sm\">\n          <span class=\"text-primary\">检测到剪贴板包含 {{ clipboardSuggestion.type }} 数据，</span>\n          <button \n            class=\"text-primary font-medium underline ml-1\"\n            @click=\"goToTool(clipboardSuggestion.toolId)\"\n          >\n            是否打开 {{ clipboardSuggestion.toolName }}？\n          </button>\n        </div>\n        <button \n          class=\"text-muted-foreground hover:text-foreground\"\n          @click=\"dismissClipboardSuggestion\"\n        >\n          <X class=\"w-4 h-4\" />\n        </button>\n      </div>\n      \n      <!-- 热门/快捷标签 -->\n      <div class=\"flex flex-wrap justify-center gap-2\">\n        <span class=\"text-xs text-muted-foreground\">热门搜索：</span>\n        <button\n          v-for=\"tag in popularTags\"\n          :key=\"tag\"\n          class=\"text-xs px-3 py-1 rounded-full bg-muted/30 hover:bg-primary/15 hover:text-primary hover:border-primary/40 hover:scale-105 transition-all duration-200 border border-border/50 shadow-sm hover:shadow-md\"\n          @click=\"setSearchTerm(tag)\"\n        >\n          {{ tag }}\n        </button>\n      </div>\n    </div>\n    </div>\n    \n    <!-- 个性化工作台 (最近使用/收藏) -->\n    <section class=\"mb-16\">\n      <div class=\"flex items-center justify-between mb-6\">\n        <h2 class=\"text-2xl font-bold flex items-center gap-2\">\n          <Clock class=\"w-6 h-6 text-primary\" />\n          最近使用\n        </h2>\n        <button \n          v-if=\"recentTools.length > 0\"\n          class=\"text-sm text-muted-foreground hover:text-primary flex items-center gap-1\"\n          @click=\"clearRecentToolsHandler\"\n        >\n          <Trash2 class=\"w-4 h-4\" />\n          清空记录\n        </button>\n      </div>\n      \n      <div v-if=\"recentTools.length > 0\" class=\"grid grid-cols-2 sm:grid-cols-3 md:grid-cols-4 lg:grid-cols-6 gap-4\">\n        <NuxtLink\n          v-for=\"tool in recentTools\"\n          :key=\"tool.id\"\n          :to=\"`/tools/${tool.id}/`\"\n          class=\"group relative block\"\n          @click=\"addRecentTool(tool.id)\"\n        >\n          <div class=\"bg-card/30 backdrop-blur-sm border-0 rounded-xl p-4 hover:bg-card/60 hover:shadow-sm hover:shadow-primary/5 transition-all duration-200 cursor-pointer group h-full\">\n            <div class=\"flex flex-col items-center text-center h-full\">\n              <component\n                :is=\"getIconComponent(tool.icon)\"\n                class=\"w-8 h-8 mb-2 transition-transform group-hover:scale-110\"\n                :style=\"{ color: getCategoryColor(tool.category).icon }\"\n              />\n              <span class=\"text-sm font-medium truncate w-full\">{{ tool.name }}</span>\n            </div>\n          </div>\n        </NuxtLink>\n      </div>\n      \n      <!-- 新用户显示推荐工具 -->\n      <div v-else class=\"grid grid-cols-2 sm:grid-cols-3 md:grid-cols-4 lg:grid-cols-6 gap-4\">\n        <NuxtLink\n          v-for=\"tool in recommendedTools\"\n          :key=\"tool.id\"\n          :to=\"`/tools/${tool.id}/`\"\n          class=\"group relative block\"\n          @click=\"addRecentTool(tool.id)\"\n        >\n          <div class=\"bg-card/30 backdrop-blur-sm border-0 rounded-xl p-4 hover:bg-card/60 hover:shadow-sm hover:shadow-primary/5 transition-all duration-200 cursor-pointer group h-full\">\n            <div class=\"flex flex-col items-center text-center h-full\">\n              <component\n                :is=\"getIconComponent(tool.icon)\"\n                class=\"w-8 h-8 mb-2 transition-transform group-hover:scale-110\"\n                :style=\"{ color: getCategoryColor(tool.category).icon }\"\n              />\n              <span class=\"text-sm font-medium truncate w-full\">{{ tool.name }}</span>\n            </div>\n          </div>\n        </NuxtLink>\n      </div>\n    </section>\n    \n    <!-- 分类导航展架 -->\n    <section class=\"mb-16\">\n      <div \n        v-for=\"category in categoriesWithTools\" \n        :key=\"category.id\"\n        class=\"mb-12 scroll-mt-24\"\n        :id=\"`category-${category.id}`\"\n      >\n        <h2 class=\"text-2xl font-bold mb-6 flex items-center gap-2\">\n          <component\n            :is=\"getIconComponent(category.icon)\"\n            class=\"w-6 h-6\"\n            :style=\"{ color: getCategoryColor(category.id).icon }\"\n          />\n          {{ category.name }}\n        </h2>\n        \n        <div class=\"grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4\">\n          <NuxtLink\n            v-for=\"tool in category.tools.slice(0, 6)\"\n            :key=\"tool.id\"\n            :to=\"`/tools/${tool.id}/`\"\n            class=\"group relative block\"\n            @click=\"addRecentTool(tool.id)\"\n          >\n            <div class=\"bg-card/40 backdrop-blur-sm border-0 rounded-xl p-5 hover:bg-card/70 hover:shadow-lg hover:shadow-primary/8 transition-all duration-200 cursor-pointer group h-full\">\n              <div class=\"flex justify-between items-start mb-3\">\n                <component\n                  :is=\"getIconComponent(tool.icon)\"\n                  class=\"w-8 h-8 group-hover:scale-110 transition-transform\"\n                  :style=\"{ color: getCategoryColor(tool.category).icon }\"\n                />\n                <span\n                  v-if=\"tool.hot || tool.new\"\n                  :class=\"[\n                    'text-xs px-2 py-1 rounded-full',\n                    tool.hot ? 'bg-red-500/20 text-red-400' : 'bg-green-500/20 text-green-400'\n                  ]\"\n                >\n                  {{ tool.hot ? 'HOT' : 'NEW' }}\n                </span>\n              </div>\n              <h3 class=\"font-semibold mb-2\">{{ tool.name }}</h3>\n              <p class=\"text-sm text-muted-foreground\">{{ tool.description }}</p>\n            </div>\n          </NuxtLink>\n        </div>\n      </div>\n    </section>\n    \n    <!-- 特性与信任背书 -->\n    <section class=\"mb-16\">\n      <div class=\"grid grid-cols-1 md:grid-cols-3 gap-6\">\n        <div class=\"bg-card border border-border rounded-xl p-6 text-center hover:shadow-lg transition-shadow\">\n          <div class=\"w-12 h-12 bg-primary/10 rounded-lg flex items-center justify-center mb-4 mx-auto\">\n            <Lock class=\"w-6 h-6 text-primary\" />\n          </div>\n          <h3 class=\"font-semibold mb-2\">纯本地计算</h3>\n          <p class=\"text-sm text-muted-foreground\">您的数据不会上传到服务器，所有计算均在浏览器本地完成，安全隐私。</p>\n        </div>\n        \n        <div class=\"bg-card border border-border rounded-xl p-6 text-center hover:shadow-lg transition-shadow\">\n          <div class=\"w-12 h-12 bg-primary/10 rounded-lg flex items-center justify-center mb-4 mx-auto\">\n            <Zap class=\"w-6 h-6 text-primary\" />\n          </div>\n          <h3 class=\"font-semibold mb-2\">极速响应</h3>\n          <p class=\"text-sm text-muted-foreground\">WebAssembly加速，毫秒级处理速度，无需等待服务器响应。</p>\n        </div>\n        \n        <div class=\"bg-card border border-border rounded-xl p-6 text-center hover:shadow-lg transition-shadow\">\n          <div class=\"w-12 h-12 bg-primary/10 rounded-lg flex items-center justify-center mb-4 mx-auto\">\n            <Heart class=\"w-6 h-6 text-primary\" />\n          </div>\n          <h3 class=\"font-semibold mb-2\">开源免费</h3>\n          <p class=\"text-sm text-muted-foreground\">无广告，社区驱动，永久免费，GitHub开源项目。</p>\n        </div>\n      </div>\n    </section>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed, onMounted, onUnmounted } from 'vue'\nimport { useRouter } from 'vue-router'\nimport {\n  Search, Clock, Trash2, X, Lock, Zap, Heart,\n  FileJson, FileText, Database, Link, Hash, Timer, Regex, FileDiff,\n  Globe, FolderOpen, GitBranch, Type, Wifi, Image, Code,\n  Shield, Layout\n} from 'lucide-vue-next'\nimport { categories } from '~/data/categories'\nimport { tools } from '~/data/tools'\nimport { initToolsData, getRecentTools, addRecentTool, clearRecentTools } from '~/composables/useTools'\n\ndefinePageMeta({\n  layout: 'default'\n})\n\nimport { useSEO } from '~/composables/useSEO'\n\n// 使用SEO composable设置页面标题\nconst { setPageTitle } = useSEO()\nsetPageTitle('Util.cn - 开发者效率工具箱')\n\n\nconst router = useRouter()\n\n// 初始化工具数据\ninitToolsData()\n\n// 数据状态\nconst searchQuery = ref('')\nconst clipboardSuggestion = ref(null)\n\n// 图标映射\nconst iconMap = {\n  FileText: FileText,\n  FileJson: FileJson,\n  Lock: Lock,\n  Shield: Shield,\n  Clock: Clock,\n  Type: Type,\n  Wifi: Wifi,\n  Image: Image,\n  Code: Code,\n  Database: Database,\n  Link: Link,\n  Hash: Hash,\n  Timer: Timer,\n  Regex: Regex,\n  FileDiff: FileDiff,\n  Globe: Globe,\n  FolderOpen: FolderOpen,\n  GitBranch: GitBranch,\n  Layout: Layout\n}\n\n// 热门标签\nconst popularTags = ref([\n  'JSON格式化',\n  '时间戳转换',\n  'Base64编码',\n  'Cron生成器',\n  'JWT解码',\n  'URL编码'\n])\n\n// 推荐工具（新用户）\nconst recommendedTools = computed(() => {\n  // 选择一些热门工具作为推荐\n  return tools.filter(tool => tool.hot).slice(0, 6)\n})\n\n// 最近使用的工具\nconst recentTools = computed(() => {\n  return getRecentTools()\n    .map(id => tools.find(tool => tool.id === id))\n    .filter(Boolean)\n    .slice(0, 6)\n})\n\n// 分类及工具\nconst categoriesWithTools = computed(() => {\n  return categories.map(category => ({\n    ...category,\n    tools: tools.filter(tool => tool.category === category.id)\n  }))\n})\n\n// 搜索输入引用\nconst searchInput = ref(null)\n\n// 处理搜索\nconst handleSearch = (event) => {\n  searchQuery.value = event.target.value\n}\n\n// 处理回车搜索\nconst handleSearchEnter = () => {\n  if (searchQuery.value.trim()) {\n    // 跳转到搜索结果页面或过滤工具\n    router.push(`/all/?search=${encodeURIComponent(searchQuery.value)}`)\n  }\n}\n\n// 设置搜索词\nconst setSearchTerm = (term) => {\n  searchQuery.value = term\n  handleSearchEnter()\n}\n\n// 跳转到工具\nconst goToTool = (toolId) => {\n  const tool = tools.find(t => t.id === toolId)\n  if (tool) {\n    // 使用目录结构格式\n    const toolUrl = `/tools/${tool.id}/`\n    router.push(toolUrl)\n    addRecentTool(tool.id)\n  }\n}\n\n\n// 清空最近使用的工具\nconst clearRecentToolsHandler = () => {\n  clearRecentTools()\n}\n\n// 关闭剪贴板提示\nconst dismissClipboardSuggestion = () => {\n  clipboardSuggestion.value = null\n}\n\n// 获取图标组件\nconst getIconComponent = (iconName) => {\n  const defaultIcon = iconMap['FileText'] || iconMap['FileJson']\n  return iconMap[iconName] || defaultIcon\n}\n\n// 导入通用颜色配置\nimport { getCategoryColor as getCatColor } from '~/utils/categoryColors'\n\n// 获取分类对应的霓虹色彩\nconst getCategoryColor = getCatColor\n\n// 检测剪贴板内容\nconst detectClipboardContent = async () => {\n  try {\n    const text = await navigator.clipboard.readText()\n    \n    // 检测不同类型的内容\n    if (text.startsWith('{') && text.endsWith('}')) {\n      try {\n        JSON.parse(text)\n        clipboardSuggestion.value = {\n          type: 'JSON',\n          toolId: 'json-formatter',\n          toolName: 'JSON格式化'\n        }\n        return\n      } catch (e) {\n        // 不是有效的JSON\n      }\n    }\n    \n    // 检测时间戳\n    if (/^\\d{10,13}$/.test(text)) {\n      clipboardSuggestion.value = {\n        type: '时间戳',\n        toolId: 'unix-timestamp',\n        toolName: 'Unix时间戳转换'\n      }\n      return\n    }\n    \n    // 检测Base64\n    if (/^[A-Za-z0-9+/]*={0,2}$/.test(text) && text.length > 10) {\n      clipboardSuggestion.value = {\n        type: 'Base64',\n        toolId: 'base64-encoder',\n        toolName: 'Base64编码/解码'\n      }\n      return\n    }\n    \n    // 检测JWT\n    if (text.startsWith('eyJ') && text.includes('.')) {\n      clipboardSuggestion.value = {\n        type: 'JWT',\n        toolId: 'jwt-decoder',\n        toolName: 'JWT解码'\n      }\n      return\n    }\n  } catch (error) {\n    // 无法读取剪贴板，静默处理\n    console.log('无法读取剪贴板内容')\n  }\n}\n\n// 处理粘贴事件\nconst handlePaste = (event) => {\n  // 延迟一段时间后检测剪贴板内容\n  setTimeout(detectClipboardContent, 100)\n}\n\n// 在组件挂载时初始化\nonMounted(() => {\n  // 添加粘贴事件监听器\n  window.addEventListener('paste', handlePaste)\n  \n  // 页面加载时检测一次剪贴板\n  detectClipboardContent()\n})\n\n// 在组件卸载时移除事件监听器\nonUnmounted(() => {\n  window.removeEventListener('paste', handlePaste)\n})\n</script>\n\n<style scoped>\n/* 呼吸动画效果 */\n@keyframes breathe {\n  0%, 100% {\n    transform: translate(0, 0) scale(1);\n    opacity: 0.15;\n  }\n  50% {\n    transform: translate(-10px, -10px) scale(1.1);\n    opacity: 0.25;\n  }\n}\n\n.animate-breathe {\n  animation: breathe 8s ease-in-out infinite;\n}\n\n/* 慢速脉冲动画 */\n@keyframes pulse-slow {\n  0%, 100% {\n    opacity: 0.08;\n    transform: scale(1);\n  }\n  50% {\n    opacity: 0.12;\n    transform: scale(1.05);\n  }\n}\n\n.animate-pulse-slow {\n  animation: pulse-slow 4s ease-in-out infinite;\n}\n\n/* 延迟动画类 */\n.delay-1000 {\n  animation-delay: 1s;\n}\n\n.delay-2000 {\n  animation-delay: 2s;\n}\n\n.delay-4000 {\n  animation-delay: 4s;\n}\n\n/* 确保背景元素不影响布局 */\n.absolute.inset-0.-z-10 {\n  pointer-events: none;\n}\n</style>"],"version":3}